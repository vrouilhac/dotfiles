alias g='git'

alias ga='git add'

alias gb='git branch'

alias gc='git commit -v'
alias gcm='git commit -v -m'
alias gca='git commit -v --amend'

alias gco='git checkout'
alias gcob='git checkout -b'

alias gd='git diff'
alias gs='git status'

alias gf='git fetch'
alias gfa='git fetch --all --prune'

alias gpl='git pull'

alias gp='git push'
alias gpu='git push --set-upstream origin'

alias gm='git merge'
